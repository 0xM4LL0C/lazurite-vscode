class Calc {
    func Calc(a, op, b){
        this.a = a
        this.b = b
        this.op = op
    }

    func calc() {
        return this._calc(this.a, this.op, this.b)
    }

    func _calc(a, op, b){
        return match (op){
            case "+": a + b
            case "-": a - b
            case "*": a * b
            case "/": a / b
            case _: {
                throw ValueError "Unsupported op: `" + op + "`"
            }

        }
    }
}


